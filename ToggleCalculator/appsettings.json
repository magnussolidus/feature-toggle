{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "FeatureManagement": {
    "AllowSum": {
      "EnabledFor": [
        {
          "Name": "AlwaysOn"
        }
      ]
    },
    "AllowSubtraction": true,
    "AllowNegativeNumbers": false,
    "AllowMultiplication": true,
    "AllowDivision": true,
    "CacheFlags": false
  }
}